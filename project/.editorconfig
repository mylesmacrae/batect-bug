root = true ; top-most EditorConfig file


; Windows-style newlines with 4 space indentation

[*]
indent_style = space
indent_size = 4
end_of_line = crlf

[*.{css,js,json,scss}]
indent_style = tab
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = false

[*.xslt]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
